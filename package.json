{
	"name": "theme-arghkai-vscode",
	"displayName": "Arghkai",
	"description": "Stolen't",
	"publisher": "jmg-duarte",
	"version": "1.1.19",
	"engines": {
		"vscode": "^1.14.0"
	},
	"categories": [
		"Themes"
	],
	"contributes": {
		"themes": [
			{
				"label": "Arghkai",
				"uiTheme": "vs-dark",
				"path": "./themes/Arghkai.json"
			},
			{
				"label": "Arghkai (Filter Octagon)",
				"uiTheme": "vs-dark",
				"path": "./themes/Arghkai (Filter Octagon).json"
			},
			{
				"label": "Arghkai (Filter Ristretto)",
				"uiTheme": "vs-dark",
				"path": "./themes/Arghkai (Filter Ristretto).json"
			},
			{
				"label": "Arghkai (Filter Spectrum)",
				"uiTheme": "vs-dark",
				"path": "./themes/Arghkai (Filter Spectrum).json"
			},
			{
				"label": "Arghkai (Filter Machine)",
				"uiTheme": "vs-dark",
				"path": "./themes/Arghkai (Filter Machine).json"
			},
			{
				"label": "Arghkai Classic",
				"uiTheme": "vs-dark",
				"path": "./themes/Arghkai Classic.json"
			}
		],
		"iconThemes": [
			{
				"label": "Arghkai Icons",
				"id": "Arghkai Icons",
				"path": "./icon-themes/Arghkai icon-theme.json"
			},
			{
				"label": "Arghkai Monochrome Icons",
				"id": "Arghkai Monochrome Icons",
				"path": "./icon-themes/Arghkai monochrome icon-theme.json"
			},
			{
				"label": "Arghkai (Filter Octagon) Icons",
				"id": "Arghkai (Filter Octagon) Icons",
				"path": "./icon-themes/Arghkai (Filter Octagon) icon-theme.json"
			},
			{
				"label": "Arghkai (Filter Octagon) Monochrome Icons",
				"id": "Arghkai (Filter Octagon) Monochrome Icons",
				"path": "./icon-themes/Arghkai (Filter Octagon) monochrome icon-theme.json"
			},
			{
				"label": "Arghkai (Filter Ristretto) Icons",
				"id": "Arghkai (Filter Ristretto) Icons",
				"path": "./icon-themes/Arghkai (Filter Ristretto) icon-theme.json"
			},
			{
				"label": "Arghkai (Filter Ristretto) Monochrome Icons",
				"id": "Arghkai (Filter Ristretto) Monochrome Icons",
				"path": "./icon-themes/Arghkai (Filter Ristretto) monochrome icon-theme.json"
			},
			{
				"label": "Arghkai (Filter Spectrum) Icons",
				"id": "Arghkai (Filter Spectrum) Icons",
				"path": "./icon-themes/Arghkai (Filter Spectrum) icon-theme.json"
			},
			{
				"label": "Arghkai (Filter Spectrum) Monochrome Icons",
				"id": "Arghkai (Filter Spectrum) Monochrome Icons",
				"path": "./icon-themes/Arghkai (Filter Spectrum) monochrome icon-theme.json"
			},
			{
				"label": "Arghkai (Filter Machine) Icons",
				"id": "Arghkai (Filter Machine) Icons",
				"path": "./icon-themes/Arghkai (Filter Machine) icon-theme.json"
			},
			{
				"label": "Arghkai (Filter Machine) Monochrome Icons",
				"id": "Arghkai (Filter Machine) Monochrome Icons",
				"path": "./icon-themes/Arghkai (Filter Machine) monochrome icon-theme.json"
			},
			{
				"label": "Arghkai Classic Icons",
				"id": "Arghkai Classic Icons",
				"path": "./icon-themes/Arghkai Classic icon-theme.json"
			},
			{
				"label": "Arghkai Classic Monochrome Icons",
				"id": "Arghkai Classic Monochrome Icons",
				"path": "./icon-themes/Arghkai Classic monochrome icon-theme.json"
			}
		],
		"configuration": {
			"type": "object",
			"title": "Arghkai Settings",
			"properties": {
				"arghkai.fileIconsMonochrome": {
					"type": "boolean",
					"default": false,
					"description": "makes the file icons monochrome"
				}
			}
		}
	},
	"devDependencies": {
		"vscode": "^1.0.0"
	},
	"extensionKind": [
		"ui",
		"workspace"
	],
	"icon": "icon.png",
	"keywords": [
		"beautiful functionality",
		"focus",
		"minimal",
		"icons",
		"syntax highlighting"
	],
	"main": "./js/main",
	"qna": false,
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install"
	}
}